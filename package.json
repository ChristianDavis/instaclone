{
	"name": "instaclone",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"android:release": "cd android && ./gradlew assembleRelease && cd ../ && mv android/app/build/outputs/apk/release/app-release.apk build/app-release.apk && adb install build/app-release.apk ",
		"ios": "react-native run-ios",
		"web:start": "react-app-rewired start",
		"start": "react-native start",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"type-check": "tsc --pretty --noEmit",
		"format": "prettier --write"
	},
	"dependencies": {
		"@gorhom/animated-tabbar": "^2.1.1",
		"@react-native-async-storage/async-storage": "^1.14.1",
		"@react-native-community/google-signin": "^5.0.0",
		"@react-native-community/masked-view": "^0.1.10",
		"@react-native-firebase/app": "^11.1.2",
		"@react-native-firebase/auth": "^11.1.2",
		"@react-native-firebase/firestore": "^11.1.2",
		"@react-native-firebase/storage": "^11.1.2",
		"@react-navigation/bottom-tabs": "^5.11.8",
		"@react-navigation/native": "^5.9.3",
		"@react-navigation/stack": "^5.14.3",
		"core-js": "2",
		"customize-cra": "^1.0.0",
		"date-fns": "^2.19.0",
		"firebase": "^8.3.1",
		"mobx": "^6.1.8",
		"mobx-react-lite": "^3.2.0",
		"mobx-state-tree": "^5.0.1",
		"nanoid": "^3.1.22",
		"react": "17.0.2",
		"react-app-rewired": "^2.1.8",
		"react-art": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-native": "0.64.0",
		"react-native-gesture-handler": "^1.10.3",
		"react-native-get-random-values": "^1.6.0",
		"react-native-image-crop-picker": "^0.36.0",
		"react-native-modal": "^11.7.0",
		"react-native-paper": "^4.7.2",
		"react-native-reanimated": "^2.0.1",
		"react-native-safe-area-context": "^3.2.0",
		"react-native-screens": "^2.18.1",
		"react-native-svg": "^12.1.0",
		"react-native-vector-icons": "^8.1.0",
		"react-native-web": "^0.15.0",
		"reactotron-mst": "^3.1.3"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.13.8",
		"@babel/preset-env": "^7.13.12",
		"@babel/preset-flow": "^7.12.13",
		"@babel/preset-react": "^7.12.13",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/runtime": "^7.13.10",
		"@react-native-community/eslint-config": "^2.0.0",
		"@types/customize-cra": "^1.0.1",
		"@types/react": "^17.0.3",
		"@types/react-native": "^0.64.1",
		"@types/react-native-vector-icons": "^6.4.6",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"babel-loader": "^8.2.2",
		"babel-plugin-import": "^1.13.3",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"file-loader": "^6.2.0",
		"husky": "^5.2.0",
		"lint-staged": "^10.5.4",
		"metro-react-native-babel-preset": "^0.65.2",
		"prettier": "^2.2.1",
		"react-scripts": "^4.0.3",
		"reactotron-react-native": "^5.0.0",
		"typescript": "^4.2.3",
		"url-loader": "^4.1.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn run type-check"
		}
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn format"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
